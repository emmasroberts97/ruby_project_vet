<form class="create" action="/vets/<%= @vet.id %>" method="post">

  <label for="name">Name:</label>
    <input id="name" type="text" name="name" value = "<%= @vet.name %>" />


    <label for="experience">Years of Experience:</label>
    <input id="experience" type="experience" name="experience" value = "<%= @vet.experience %>" />

    <label for="url">Picture:</label>
    <input id="url" type="text" name="url" value = "<%= @vet.url %>" />

    <input class="submit-button" type='submit' value="Update Vet"/>

</form>
